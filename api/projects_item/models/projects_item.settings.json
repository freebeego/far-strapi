{
  "kind": "collectionType",
  "collectionName": "projects_item",
  "info": {
    "name": "Projects item"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title_ru": {
      "type": "string",
      "required": true
    },
    "title_en": {
      "type": "string",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "description_ru": {
      "type": "text",
      "required": true
    },
    "description_en": {
      "type": "text",
      "required": true
    },
    "date_ru": {
      "type": "string",
      "required": true
    },
    "date_en": {
      "type": "string",
      "required": true
    }
  }
}
